+  Upcase extends boolean | undefined = undefined,
+> = Upcase extends undefined ? T : Uppercase<T>
+
+type NetworkName<
+  TNetworkId extends number | undefined = undefined,
+  Upcase extends boolean | undefined = undefined,
+> = TNetworkId extends undefined
+  ?
